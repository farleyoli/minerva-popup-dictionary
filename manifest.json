{
  "name": "Minerva Pop-up Dictionary",
  "version": "1.0",
  "description": "English dictionary to be used with Chrome!",
  "permissions":
  ["activeTab",
      "storage",
      "tabs",
      "contextMenus",
      "<all_urls>",
      "clipboardWrite",
      "unlimitedStorage",
      "webRequestBlocking",
      "http://127.0.0.1:8765"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"],
      "css": ["popup.css"]
    }
  ],
  "web_accessible_resources": [
      "dictionary/*.json",
      "http://127.0.0.1:*"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "icons": {
  },
  "options_page": "options.html",
  "manifest_version": 2
}
